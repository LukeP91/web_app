- model_class = Category

.page-header
  %h1=t '.title', default: [:'helpers.titles.new', 'New %{model}'], model: model_class.model_name.human.titleize

= render partial: "form", locals: { category: category }

= link_to t('.cancel', default: t("helpers.links.cancel")), admin_categories_path, class: 'btn btn-default'
